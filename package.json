  "scripts": {
    "start": "node src/index.js",
    "daily": "node src/jobs/daily.js",
    "daily:test": "node src/jobs/daily-test.js",
    "daily:full": "node src/jobs/daily.js",
    "weekly": "node src/jobs/weekly.js",
    "monthly": "node src/jobs/monthly.js",
    "monthly:test": "node src/jobs/monthly.js",
    "dashboard": "node src/dashboard/server.js",
    "validate": "node src/validate-env.js",
